{
  "name": "server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "babel-node bin/www",
    "dev": "nodemon --exec \"npm start\"",
    "debug": "babel-node --inspect-brk bin/www",
    "prd": "pm2 start bin/www",
    "build": "npm run build:copy && npm run build:transpile",
    "build:copy": "cp bin/www package.json package-lock.json logs/.gitkeep -t dist --parents",
    "build:transpile": "babel --out-dir dist --ignore node_modules ./"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "debug": "^2.6.3",
    "ioredis": "^3.2.2",
    "koa": "^2.2.0",
    "koa-better-body": "^3.0.4",
    "koa-body-parsers": "^3.1.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^2.0.1",
    "koa-onerror": "^1.2.1",
    "koa-router": "^7.1.1",
    "koa-session2": "^2.2.6",
    "lodash": "^4.17.10",
    "md5-nodejs": "^1.0.3",
    "raw-body": "^2.3.3",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "session-file-store": "^1.3.0",
    "ssh2-sftp-client": "^2.4.3",
    "svg-captcha": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.16.0",
    "nodemon": "^1.8.1"
  }
}
